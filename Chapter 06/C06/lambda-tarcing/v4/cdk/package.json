{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk:run": "cdk --profile serverless --app 'npx ts-node --prefer-ts-exts bin/cdk.ts'",
    "cdk:synth": "npm run cdk:run synth",
    "cdk:deploy": "npm run cdk:run deploy",
    "cdk:destroy": "npm run cdk:run  destroy"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "20.10.4",
    "aws-cdk": "2.118.0",
    "esbuild": "^0.20.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.118.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
